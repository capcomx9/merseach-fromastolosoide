            #$96#$EB#$96#$B3#$22#$78#$1B#$39#$A0#$13#$A9#$84#$E5#$E6#$33#$E7#$66+
            #$0B#$34#$97#$3B#$B1#$BE#$72#$3F#$20#$A1#$61#$D6#$4F#$90#$AB#$80#$85+
            #$9A#$3C#$3F#$76#$50#$BC#$6A#$AC#$5E#$85#$7E#$E6#$85#$09#$38#$23#$B8+
            #$C9#$69#$AD#$62#$A3);
       Write(#$4C#$B4#$EC#$BE#$77#$9B#$BD#$E3#$9B#$49#$84#$7B#$D6#$2E#$6E#$56#$6E+
            #$60#$45#$4B#$5F#$13#$B6#$59#$BC#$3D#$9A#$51#$1B#$4E#$B9#$E2#$BD#$A1+
            #$E0#$06#$23#$F9#$E4#$02#$05#$96#$63#$AF#$B3#$A8#$9E#$F2#$D5#$0C#$09+
            #$17#$E1#$5F#$7B#$D5#$28#$23#$75#$AD#$7D#$52#$4C#$88#$2E#$7F#$E7#$DE+
            #$9A#$76#$19#$05#$9D#$3C#$26#$BD#$0A#$D3#$96#$FF#$9F#$E3#$93#$E0#$79+
            #$4F#$59#$78#$37#$8B#$BD#$35#$05#$72#$61#$05#$CB#$AA#$C5#$FE#$65#$6E+
            #$49#$12#$1E#$15#$52#$71#$3A#$41#$35#$4C#$D1#$C8#$9C#$74#$B0#$F9#$8E+
            #$80#$CB#$25#$03#$90#$4E#$ED#$85#$E8#$C7#$09#$15#$F3#$07#$82#$F3#$9A+
            #$1E#$4F#$27#$FD#$85#$94#$A1#$22#$6E#$9E#$1E#$22#$56#$06#$46#$02#$09+
            #$FB#$09#$AA#$77#$4B#$2D#$30#$09#$51#$03#$BD#$14#$C2#$47#$82);
      FixSize;
      Encrypt(GenerateIV(0),Random($FF));
      ClearPacket();
    end;
  Player.Send;
  End;


end.
